{
  "name": "@fouaden/snipit",
  "version": "1.1.0",
  "description": "A lightweight CLI tool to save, search and tag code snippets locally. Stop re-typing; start snipping.",
  "main": "bin/snipit.js",
  "scripts": {
    "dev": "tsx bin/snipit.js",
    "build": "tsc",
    "start": "node bin/snipit.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "bin": {
    "snipit": "./bin/snipit.js"
  },
  "files": [
    "dist/",
    "bin/"
  ],
  "keywords": [
    "cli",
    "snipit",
    "snippet-manager",
    "developer-tools",
    "typescript",
    "productivity",
    "code-snippets",
    "terminal",
    "local-storage",
    "node-cli"
  ],
  "author": "fouadbuilds",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.3.0",
    "clipboardy": "^5.3.1",
    "commander": "^14.0.3",
    "picocolors": "^1.1.1"
  }
}
